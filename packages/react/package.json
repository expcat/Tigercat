{
  "name": "@tigercat/react",
  "version": "0.0.1",
  "description": "React components for Tigercat UI library",
  "license": "MIT",
  "author": "Yizhe Wang",
  "repository": {
    "type": "git",
    "url": "https://github.com/expcats/Tigercat.git",
    "directory": "packages/react"
  },
  "keywords": [
    "tigercat",
    "ui",
    "react",
    "components"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format cjs,esm --dts --external react --external react-dom --external react-dom/client",
    "dev": "tsup src/index.tsx --format cjs,esm --dts --external react --external react-dom --external react-dom/client --watch",
    "clean": "node ../../scripts/rimraf.mjs dist"
  },
  "dependencies": {
    "@tigercat/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
